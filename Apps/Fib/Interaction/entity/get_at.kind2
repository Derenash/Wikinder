#kdl_name = Fib_itr_egat
Apps.Fib.Interaction.entity.get_at (pos: Apps.Fib.Pos ) : Apps.Fib.Interaction Apps.Fib.Player {
  do Apps.Fib.Interaction {
    ask id = Apps.Fib.Interaction.entity.get_id_at pos
    ask table = Apps.Fib.Interaction.table.get
    let entity = Apps.Fib.Table.get table id
    return entity
  }
}