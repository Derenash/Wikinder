Apps.Fib.Interaction.entity.mut_at (func: Apps.Fib.Entity -> Apps.Fib.Entity) (pos: Apps.Fib.Pos): Apps.Fib.Interaction Unit {
  do Apps.Fib.Interaction {
    ask entity_id = Apps.Fib.Interaction.entity.get_id_at pos
    let func = (table: Apps.Fib.Table) => Apps.Fib.Table.mut table func entity_id
    ask Apps.Fib.Interaction.table.mut func
    return Unit.new
  }
}