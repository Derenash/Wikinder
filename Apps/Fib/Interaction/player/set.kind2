#kdl_name = Fib_itr_pset
Apps.Fib.Interaction.player.set (new_player: Apps.Fib.Player) : Apps.Fib.Interaction Unit {
  do Apps.Fib.Interaction {
    ask player_id = Apps.Fib.Interaction.player.get_id
    let func = (table: Apps.Fib.Table) => Apps.Fib.Table.set table new_player player_id
    ask Apps.Fib.Interaction.table.mut func
    return Unit.new
  }
}