Apps.Fib.Npc.ignis : Apps.Fib.Interaction Unit {
  do Apps.Fib.Interaction {
    ask player = Apps.Fib.Interaction.player.get
    let slot = Apps.Fib.Player.skills.has_slot player
    let ignis_stone = (U120.new 0 27)
    match Maybe slot {
      none => Apps.Fib.Interaction.fail
      some => do Apps.Fib.Interaction {
        ask Apps.Fib.Interaction.skill.add ignis_stone
        return Unit 
      }
    }
  }
}