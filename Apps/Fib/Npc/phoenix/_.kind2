#kdl_name = Fib_npc_phnx
Apps.Fib.Npc.phoenix : Apps.Fib.Interaction Unit {
  do Apps.Fib.Interaction {
    ask frame     = Apps.Fib.Interaction.frame.get
    ask duration  = Apps.Fib.Interaction.entity.get
    let is_active = U120.greater_equal duration frame
                    Apps.Fib.Interaction.check is_active
    Apps.Fib.Npc.phoenix.fruit
  }
}
