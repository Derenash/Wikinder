#kdl_name = Fib_npc_phpr
Apps.Fib.Npc.phoenix.parse (entity: Apps.Fib.Entity) (frame: U120) : Apps.Fib.Attributes {
  let duration = U120.sub_saturated entity frame
  [
    (Pair.new (Kindelia.Convert.to_kstring "type")     (Kindelia.Convert.to_kstring "phoenix"))
    (Pair.new (Kindelia.Convert.to_kstring "duration") (Kindelia.Convert.num_to_kstring duration)) 
  ]
}