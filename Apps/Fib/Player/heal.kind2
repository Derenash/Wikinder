#kdl_name = Fib_plr_heal
Apps.Fib.Player.heal (player: Apps.Fib.Player) (heal: U120) : Apps.Fib.Player {
  let overflow = ((x: U120) => 
    let result = U120.add heal x
    if (U120.greater_than x result) {
      let xp = Apps.Fib.Player.xp.get player
      Apps.Fib.Player.hp.max xp
      } else {
      result
    }
  )
  Apps.Fib.Player.hp.change player ((x : U120) => overflow x)
}
