#kdl_name = Fib_plr_heal
Apps.Fib.Player.heal (player: Apps.Fib.Player) (heal: U120) : Apps.Fib.Player {
  let apply = ((old_hp: U120) => 
    let result = U120.add heal old_hp
    let max_hp = Apps.Fib.Player.hp.get_max player
    U120.min result max_hp
  )
  Apps.Fib.Player.hp.change player ((x : U120) => apply x)
}
