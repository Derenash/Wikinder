// is_zero should be changed to is_neg
Apps.Fib.Pos.Delta.show (delta: Pair U120 U120) : String {
  let func = (x: U120) =>
    if (U120.is_zero x ) {
      String.concat " -" ((U120.show x) String.nil)
    }
    else {
      String.cons ' ' ((U120.show x) String.nil)
    }
  String.concat (String.concat (String.cons '(' (func (Pair.fst delta))) (String.cons ',' (func (Pair.snd delta)))) ") "
}