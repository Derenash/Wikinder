  Apps.Fib.Pos.Delta.test_aux1 x_axis y_axis ""
}


Apps.Fib.Pos.kekw (pos: Apps.Fib.Pos) : Pair U60 U60 {
  let j = U120.low (Sigma.fst pos)
  let x = (% j 256)
  let y = (>> j 8)
  Pair.new x y
}

