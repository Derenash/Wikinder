#kdl_name = BrTree_crrow
Data.BraunTree.create_rows <a> (k: U120) (xs: List a) : List (Pair U120 (List a))
Data.BraunTree.create_rows a k (List.nil r)            = List.nil
Data.BraunTree.create_rows a k xs = 
  let head = Pair.new k (List.take xs (U120.to_nat k))
  let tail = Data.BraunTree.create_rows (U120.mul k (U120.new 0 2)) (List.drop xs (U120.to_nat k))
  List.cons head tail

